<div class="login">
  <div class="container">
    <div class="form">

      <h1>Iniciar Sesión</h1>
      <%= form_for(:session, url: login_path) do |f| %>
          <%= f.email_field :email, :placeholder => "Correo" %>
          <%= f.password_field :password, :placeholder => "Contraseña" %>
          <%= f.submit "Iniciar Sesión", class: "btn-submit" %>
      <% end %>


    </div>
  </div>
</div>